                            FontSize="16"
                            Foreground="Red"
                            Text="{TemplateBinding ContentControl.Content}" />
                        <Path
                            Grid.Column="1"
                            Height="10"
                            Data="{StaticResource heartIcon}"
                            Fill="Red"
                            Stretch="Uniform" />
                    </Grid>
                </ControlTemplate>
            </Setter>
