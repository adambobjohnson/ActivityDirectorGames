        <Style Selector="Border.BingoRedLetter">
            <Setter Property="Cursor" Value="Arrow" />
            <Setter Property="BorderBrush" Value="Red" />
        </Style>
        <Style Selector="Border.BingoBlueLetter">
            <Setter Property="Cursor" Value="Arrow" />
            <Setter Property="BorderBrush" Value="Blue" />
        </Style>
        <Style Selector="Border.BingoIsBlackout">
            <Setter Property="Cursor" Value="Arrow" />
            <Setter Property="BorderBrush" Value="{DynamicResource TextControlForeground}" />
        </Style>
        <Style Selector="Viewbox.Border.ThemeVariantScope">
            <Setter Property="Cursor" Value="Hand" />
        </Style>
    </UserControl.Styles>
