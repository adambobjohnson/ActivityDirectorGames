                        <Label>O</Label>
                        <Label>D</Label>
                        <Label>D</Label>
                        <Label>S</Label>
						<Label />
						<Label />
                        <Label >F</Label>
						<Label >R</Label>
						<Label >E</Label>
						<Label >E</Label>
                    </StackPanel>
                    <StackPanel
                        x:Name="EvensStack"
                        VerticalAlignment="Center"
                        IsVisible="False"
